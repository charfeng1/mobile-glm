# Zhipu API key for AutoGLM phone executor
# Get from: https://open.bigmodel.cn/
ZHIPU_API_KEY=your_zhipu_api_key_here

# Anthropic-compatible API for Claude Agent SDK (agent_sdk.py)
# Option 1: Use z.ai (Zhipu) as Anthropic-compatible endpoint
ANTHROPIC_API_KEY=your_zhipu_api_key_here
ANTHROPIC_AUTH_TOKEN=your_zhipu_api_key_here
ANTHROPIC_BASE_URL=https://api.z.ai/api/anthropic
ANTHROPIC_DEFAULT_HAIKU_MODEL=glm-4.7
ANTHROPIC_DEFAULT_SONNET_MODEL=glm-4.7
ANTHROPIC_DEFAULT_OPUS_MODEL=glm-4.7

# Option 2: Use real Anthropic API (comment out z.ai settings above)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# z.ai MCP server for vision capabilities (optional)
# Enables the agent to use z.ai's vision tools
Z_AI_API_KEY=your_zhipu_api_key_here

# Android device ID (optional - leave empty for default device)
# Get from: adb devices
PHONE_DEVICE_ID=
